on early-init

# KOR variant
on fs && property:ro.boot.em.model=SM-S901N
    mount none /vendor/firmware/kor/factory.ssc /vendor/bin/factory.ssc bind
    mount none /vendor/firmware/kor/rild /vendor/bin/hw/rild bind
    mount none /vendor/firmware/kor/secril_config_svc /vendor/bin/secril_config_svc bind
    mount none /vendor/firmware/kor/situation_audio_policy_volumes_sec.xml /vendor/etc/situation_audio_policy_volumes_sec.xml bind
