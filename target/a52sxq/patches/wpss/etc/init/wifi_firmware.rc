on early-init

# bootloader revision 1, A528N (asia)
on fs && property:ro.boot.rp=1 && property:ro.boot.em.model=SM-A528N
    mount none /vendor/firmware/wlan/a528n/rev1/wpss.b01 /vendor/firmware/wpss.b01 bind
    mount none /vendor/firmware/wlan/a528n/rev1/wpss.mdt /vendor/firmware/wpss.mdt bind

# bootloader revision 2, A528B (international)
on fs && property:ro.boot.rp=2 && property:ro.boot.em.model=SM-A528B
    mount none /vendor/firmware/wlan/a528b/rev2/wpss.b01 /vendor/firmware/wpss.b01 bind
    mount none /vendor/firmware/wlan/a528b/rev2/wpss.mdt /vendor/firmware/wpss.mdt bind
    # BL rev 2 firmware works on all rev2, rev3 and rev4 devices
    mount none /vendor/firmware/wlan/a528b/rev2/wpss.b00 /vendor/firmware/wpss.b00 bind
    mount none /vendor/firmware/wlan/a528b/rev2/wpss.b02 /vendor/firmware/wpss.b02 bind
    mount none /vendor/firmware/wlan/a528b/rev2/wpss.b03 /vendor/firmware/wpss.b03 bind
    mount none /vendor/firmware/wlan/a528b/rev2/wpss.b04 /vendor/firmware/wpss.b04 bind
    mount none /vendor/firmware/wlan/a528b/rev2/wpss.b05 /vendor/firmware/wpss.b05 bind
    mount none /vendor/firmware/wlan/a528b/rev2/wpss.b06 /vendor/firmware/wpss.b06 bind
    mount none /vendor/firmware/wlan/a528b/rev2/wpss.b07 /vendor/firmware/wpss.b07 bind

